knitr::opts_chunk$set(echo = TRUE)
## Load data
library(sas7bdat)
setwd('C:/Users/psioda/Documents/GitHub/bios-courses/BIOS-767/DATA/DENTAL')
dental.data = read.sas7bdat('dental.sas7bdat')
## change to long format
library(tidyverse)
dental.long = dental.data %>%
pivot_longer(cols = c(age8, age10, age12, age14), names_to = 'age', names_prefix = 'age', values_to = 'y')
## Run MANOVA
library(rstatix)
res.aov <- anova_test(
data = dental.long, dv = y, wid = ID,
between = GENDER, within = age
)
res.aov
