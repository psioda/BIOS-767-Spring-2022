tidyr::pivot_longer(
cols = starts_with("age"),
names_to = "age",
values_to = "result",
names_prefix = "age")
head(dental_vert)
## convert age to double precision
dental_vert$age <-
as.numeric(dental_vert$age)
head(dental_vert)
########################## ML Unstructured ###############################
attach(dental_vert)
ml.unstructured <- gls(result~0+gender+gender:age,
method="ML",
correlation=corSymm(form=~1|id),
weights=varIdent(form=~1|age))
ml.unstructured
detach(dental_vert)
